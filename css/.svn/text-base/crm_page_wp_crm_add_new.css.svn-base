@import url("jquery-ui-1.8.20.custom.css");
@import url("wp_crm_global.css");

ul {
  margin: 0;
}

/* Third Party Plugin Styles */

#poststuff #user-avatar-display  {
  position: relative;
  padding-left:170px;
  min-height: 170px
}


#poststuff #user-avatar-display h3 {
  margin-bottom: 10px;
  border:0;
}

#poststuff #user-avatar-display-image {
  margin: 0;
  padding: 0;
  position: absolute;
  top: 0px;
  left: 0px;
}

#poststuff #user-avatar-display-image   {
  border: 3px solid #FFFFFF;
  box-shadow: 1px 1px 3px #A7A7A7;
}

/* Third Party Plugin Styles */

/* Hide actions on new profile because they will most likely not work */
body.wp_crm_new_user #poststuff  .wp_crm_user_api_actions {
  display: none;
}

body.wp_crm_new_user #user_activity_history {
  display: none;
}

.wp_crm_stream_status {

}

.wp_crm_stream_status span.wp_crm_counts {
  color: #939393;
  padding: 10px;
  text-align: center;
  cursor: pointer;
  display: block;
}

div.color-option {
  clear: both;
  display: block;
  margin-top: 12px;
}

table.color-palette {
  float: left;
  margin: -12px 3px 11px;
  vertical-align: bottom;
}

table.color-palette td {
  border-color: #FFFFFF;
  border-style: solid solid none;
  border-width: 1px 1px 0;
  height: 8px;
  line-height: 15px;
  margin-bottom: 9px;
  padding: 8px 10px;
  width: 5px;
}

#poststuff  .wp_crm_user_api_actions table.form-table th,
#poststuff  .wp_crm_user_api_actions table.form-table td {
  padding: 5px 5px 5px 0;
}

#poststuff  .wp_crm_user_api_actions h3 {
  background: transparent;
  min-height: 1.7em;
  line-height: 1.7em;
  border: transparent;
  margin: 0;
  padding: 0;
  box-shadow: none;
  border-bottom: 1px dotted #C8C8C8;
}


.wp_crm_user_api_actions {
  background: none repeat scroll 0 0 #EEEEEE;
  border-top: 1px solid #E0E0E0;
  margin-bottom: -10px;
  margin-left: -10px;
  margin-right: -10px;
  overflow: auto;
  padding: 10px 10px 15px;
}


.wp_crm_secondary_ajax_result .wpc_action_wrapper {
  background: none repeat scroll 0 0 #BBCCFF;
  clear: both;
  margin-bottom: 25px;
  position: relative;
  top: 10px;
}

.wp_crm_secondary_ajax_result .wpc_action_wrapper .wpc_action_fields {
  padding: 12px 16px 6px;
}

.wp_crm_secondary_ajax_result .wpc_action_wrapper .wpc_action_fields input[type=text] {
  background: none repeat scroll 0 0 #EEF2FF;
  border: 1px solid #A1A6CC;
  border-radius: 4px 4px 4px 4px;
  padding: 1px 4px;
}

.wp_crm_secondary_ajax_result .wpc_action_wrapper .wpc_action_fields input[type=text].small_field  {
  width: 50px;
}

.wp_crm_secondary_ajax_result .wpc_action_wrapper .wpc_actions{
  background: none repeat scroll 0 0 #ADBEEF;
  padding: 9px 0 8px 10px;
}

.wp_crm_attribute_uneditable textarea,
.wp_crm_attribute_uneditable input {
  border-style:none !important;
  background: transparent !important;
}

.uploadifyQueue {
  margin-top: 10px;
}

.uploadifyQueueItem {
  background: none repeat scroll 0 0 #E4E4E4;
  border: 1px solid #BABABA;
  margin-top: 3px;
}

.uploadifyQueueItem div.cancel{
  float: left;
  margin: 3px 6px 0 1px;
}

textarea.wp_crm_input_error {
  background: none repeat scroll 0 0 #FCF2DB;
  border: 1px solid #9C3E3E !important;
}

input.regular-text.wp_crm_input_error {
  background: none repeat scroll 0 0 #FCF2DB;
  border: 1px solid #9C3E3E !important;
}

select.wp_crm_input_error {
  background: none repeat scroll 0 0 #FCF2DB;
  border: 1px solid #9C3E3E !important;
}

.wp_crm_input_error span.error{
  color: #9C3E3E;
}

div.wp_crm_description {
  color: #989898;
  font-weight: normal;
  padding-top: 5px;
}

div.wp_crm_advanced_user_actions.wp-tab-panel {
  height: auto;
  /*max-height: 200px;*/
}

.wp_crm_advanced_user_actions input.tog {
  float:left;
}

.wp-tab-panel-nav {
  margin-bottom: 3px;
  margin-top: 12px;
}
.wp-tab-panel-nav li {
  display: inline;
  line-height: 1.35em;
  border-radius: 3px 3px 0 0;
  padding: 5px;
}


.wp-tab-panel-nav li.tabs a {
  color: #333333;
  text-decoration: none;
}
.wp-tab-panel-nav li.tabs {
  border-color: #DFDFDF;
  border-style: solid solid none;
  border-width: 1px 1px 0;
  margin-bottom: 6px;
  background-color: #FFFFFF;
}

#crm_user #primary_information .wp-tab-panel {
  width: 90%;
  height: auto;
  background: #F8F8F8;
  padding:0;
}
#crm_user #primary_information .wp-tab-panel ul.wp_crm_checkbox_list {
  padding: 10px;
}
#user_activity_history.postbox .inside,
#primary_information.postbox .inside,
#bp_profile_fields.postbox .inside {
  padding: 0;
}

input.regular-text.wpc_date_picker {
  width: 8em;
}

input.regular-text {
  width: 20em;
}

body.wp_crm_my_profile .wp_crm_toggle_advanced_user_actions,
div.wp_crm_advanced_user_actions {
  display: none;
}

body.wp_crm_my_profile div.wp_crm_advanced_user_actions {
  display: block;
}

div.wp_crm_advanced_user_actions.wp-tab-panel {
  max-height: 250px;
}

.wp_crm_toggle_advanced_user_actions.wp_crm_link {
  margin-bottom: 10px;
}
#special_actions .inside {
  padding: 0;
}

#wp_crm_user_activity_stream .wp_crm_activity_single  .email_header,
#wp_crm_user_activity_stream .wp_crm_activity_single  .email_body{
  font-family: Calibri;
  line-height: 1.4em;
  font-size: 13px;
}

.wp_crm_activity_single .timestamp .time {
  display:none;
}
.wp_crm_activity_single ul.message_meta  {
  margin-left: 5px;
}


.wp_crm_activity_single .entry_type,
.wp_crm_activity_single .by_user,
.wp_crm_activity_single .timestamp {
  color: #939393;
  font-size: 11px;
  margin-top: 5px;
}

.wp_crm_activity_single td.right p.wp_crm_entry_content  {
  padding-top: 5px;
  padding-right: 10px;
}

.wp_crm_activity_single .truncated {
  background: none repeat scroll 0 0 #EFEFEF;
  padding: 5px;
}

.wp_crm_activity_single .truncated .truncated_content {
  border-left: 1px solid #CCCCCC;
  display: none;
  margin: 10px;
  padding: 10px;
}

.wp_crm_activity_single .email_header {
  background: none repeat scroll 0 0 #F5F5F5;
  margin-bottom: 5px;
  padding: 5px;
}
.wp_crm_activity_single .email_body {

}


.wp_crm_new_message_options_line {
  border-top: 1px solid #DCDCDC;
  margin-top: 7px;
  overflow: auto;
  padding-top: 5px;
}

.wp_crm_show_message_options{
  color: #000;
  cursor: pointer;
}

.wp_crm_message_options {
  margin-top: 5px;
}


#wp_crm_message_content {
  width: 100%;

}
.wp_crm_new_message {
  background: none repeat scroll 0 0 #EEEEEE;
  padding: 10px;
}


.wp_crm_activity_top {
  background: #EFEFEF;
  padding: 5px;
  overflow: auto;
  border-bottom: 1px solid #DDDDDD;

}

.wp_crm_activity_top #wp_crm_get_all_emails {
  float: right;
}

.wp_crm_activity_top .wp_crm_last_sync {
  position: relative;
  top: 7px;
  right: 15px;
  color: #B3B1B1;
  float: left;
}

#user_activity_history .inside {
  margin: 0;
}

#wp_crm_user_activity_stream {
  width: 100%;
}

.wp_crm_activity_single td {
  border-bottom: 1px solid #E0E0E0;
}

.wp_crm_activity_single .left {
  vertical-align: top;
  width: 150px;
}

.wp_crm_activity_single .right {
  vertical-align: top;

}

#side-info-column {
  width: 450px;
}

.inner-sidebar #side-sortables {
  width: 449px;
}
.has-right-sidebar #post-body  {
  margin-right: -550px;
}

.has-right-sidebar #post-body-content {
  margin-right: 471px;
}

/* Autocomplete */
ul.ui-autocomplete  {
  padding: 5px;
}
ul.ui-autocomplete .ui-menu-item {}
ul.ui-autocomplete .ui-menu-item a.ui-corner-all {}

.input_div textarea {
  width: 90%;
  padding: 5px;
  min-height: 95px;
}

#icon-wp_crm {
  background: url("images/wp-crm-icon.png") no-repeat center center;
}

#primary_information .inside,
#bp_profile_fields .inside,
.custom_group .inside {
  margin: 0 !important;
  overflow: hidden;
}

#primary_information table,
#bp_profile_fields table,
.custom_group .inside table {
  margin: 0;
}

.custom_group .inside {
  padding: 0;
}

#bp_profile_fields .wp_crm_bp_group_name {
  background: #EDEDED;
}

#bp_profile_fields .wp_crm_bp_group_name label {
  font-size: 14px;
  font-weight: bold;
  color: #414141;
}

div.blank_slate {
  color: #999999;
  cursor: pointer;
  font-size: 12px;
}


tr.wp_crm_user_entry_row td.wp_crm_user_data_row .blank_slate,
tr.wp_crm_user_entry_row td.wp_crm_user_data_row .input_div {
  margin: 4px 0 5px;
  padding: 1px;
  min-height: 25px;

}

tr.wp_crm_user_entry_row th,
tr.wp_crm_user_entry_row td {
  border-bottom: 1px solid #EDEDED;
  padding: 8px 10px 3px 10px;
}

tr.wp_crm_user_entry_row th{
  font-weight: bold;
  width: 150px;
  font-size: 12px;
}

div.add_another {
  color: #999999;
  text-decoration: none;
  cursor: pointer;
}

tr.wp_crm_user_entry_row th label{
  width: 150px;
  margin-top: 6px;
  display: block;
}
tr.wp_crm_user_entry_row select option{
  margin-right: 10px;
}

tr.wp_crm_user_entry_row.primary {
  background: #EDEDED
}

tr.wp_crm_user_entry_row.primary:last-child  {
  border-bottom: 1px solid red;
}

/* Base Styles for Input Elements */
.wp_crm_user_data_row input.regular-text,
#wpcontent .wp_crm_user_data_row select.wp_crm_dropdown,
#wpcontent .wp_crm_user_data_row select.wp_crm_input_options {
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 12px;
  height: 1.9em;
  vertical-align: middle;
}

/* Styles for Primary Elements */
tr.wp_crm_user_entry_row.primary input.regular-text,
#wpcontent tr.wp_crm_user_entry_row.primary select.wp_crm_dropdown,
#wpcontent tr.wp_crm_user_entry_row.primary .wp_crm_user_data_row select.wp_crm_input_options {
  font-size: 1.2em;
  background: #F9F9F9;
  border: 1px solid #C1C1C1;
  padding: 4px; /* needed to fix select options vertical height on FF */
}

tr.wp_crm_user_entry_row.primary div.wp_crm_uneditable_placeholder {
  font-size: 1.2em;
}

tr.wp_crm_user_entry_row.primary input.regular-text:focus,
#wpcontent tr.wp_crm_user_entry_row.primary select.wp_crm_dropdown:focus,
#wpcontent tr.wp_crm_user_entry_row.primary .wp_crm_user_data_row select.wp_crm_input_options:focus {
  background: #FFF;
  border-color: #A4A4A4;
}

.form-table input[type=checkbox] {
  width: auto;
}

#special_actions .inside {
  margin:0;
}
#special_actions #minor-publishing{
  padding: 10px;

}
.email_subject {
  margin-right: 10px;
}
.sent_date,
.email_summary {
  color: #928F8F;
}

/** BUDYPRESS FIELDS */

.bp-profile input,
.bp-profile select {
  border: 1px solid #C1C1C1;
  font-size: 1.2em;
  padding: 4px;
}

.bp-profile .readonly input,
.bp-profile .textbox input,
.bp-profile .selectbox select,
.bp-profile .multiselectbox select {
  width: 20em;
}

.bp-profile .textbox input,
.bp-profile .selectbox select,
.bp-profile .multiselectbox select {
  background: none repeat scroll 0 0 #F9F9F9;
}

.bp-profile .checkbox,
.bp-profile .radio {
  background: none repeat scroll 0 0 #F8F8F8;
  border-style: solid;
  border-width: 1px;
  width: 90%;
  padding: 10px 0 5px 0;
  border-color: #DFDFDF;
}

.bp-profile .checkbox label,
.bp-profile .radio label,
.bp-profile .bp-clear-value {
  display:block;
  padding: 0 0 5px 10px;
}

.bp-profile .multiselectbox .bp-clear-value {
  padding: 0;
}

.metabox-holder .postbox-container .empty-container, #post-body.columns-2 #side-sortables:empty {
  border: none !important;
  height: auto !important;
}

/*** WP Version 3.4 and later ***/

#poststuff.crm-wp-v34 #post-body.columns-2 {
  margin-right: 466px;
}
#poststuff.crm-wp-v34 #post-body.columns-2 #postbox-container-1 {
  margin-right: -466px;
  width: 440px;
}